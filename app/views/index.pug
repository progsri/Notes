extends layout

block content
  body
    div(class="container")
      div(class="row")
        div(class="col-md-12")
          h3= title
          br
          each resource in resources
            div
              a(href=resource["path"]) #{resource["resource"]}
              span(class=resource["color"]) #{resource["status"]}
              br
              span(class=blue) #{resource["tags"]}
   
